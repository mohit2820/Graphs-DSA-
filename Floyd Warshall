
class Solution {
    public void floydWarshall(int[][] dist) {
        
        int n = dist.length;
        int infinite = (int) 1e8;
        
        for(int k=0; k<n; k++)
        for(int i=0; i<n; i++)
        for(int j=0; j<n; j++){
            
            if(dist[i][k]==infinite || dist[k][j]==infinite) continue;
            dist[i][j] = Math.min(dist[i][j] , dist[i][k] + dist[k][j]);
        }
            
        
    }
}
